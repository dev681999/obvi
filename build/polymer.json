{
  "entrypoint": "index.html",
  "shell": "voice-button.js",
  "fragments": [],
  "sources": [
    "voice-button.js",
    "package.json",
    "index.html",
    "voice-button.js"
  ],
  "extraDependencies": [
    "bower_components/webcomponentsjs/*.js",
    "node_modules/@webcomponents/webcomponentsjs/**",
    "node_modules/@polmyer/polymer/**",
    "node_modules/@polymer/paper-behaviors/**",
    "node_modules/lz-string/**",
    "node_modules/lamejs/**"
  ],
  "builds": [
    {
      "name": "es5-bundled",
      "js": {
        "minify": true,
        "compile": "es5",
        "transformModulesToAmd": true
      },
      "css": {
        "minify": true
      },
      "html": {
        "minify": true
      },
      "bundle": true,
      "addServiceWorker": true,
      "addPushManifest": false,
      "preset": "es5-bundled"
    },
    {
      "name": "es6-bundled",
      "browserCapabilities": [
        "es2015"
      ],
      "js": {
        "minify": true,
        "compile": "es2015",
        "transformModulesToAmd": true
      },
      "css": {
        "minify": true
      },
      "html": {
        "minify": true
      },
      "bundle": true,
      "addServiceWorker": true,
      "addPushManifest": false,
      "preset": "es6-bundled"
    }
  ],
  "npm": true,
  "componentDir": "node_modules/",
  "moduleResolution": "node"
}